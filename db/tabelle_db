CREATE TABLE `temperature` (
`ID` INT NOT NULL AUTO_INCREMENT,
`timestamp` TIMESTAMP NOT NULL,
`temperature` DOUBLE NOT NULL,
PRIMARY KEY (`ID`))
ENGINE = InnoDB;

CREATE TABLE `oxygen` (
`ID` INT NOT NULL AUTO_INCREMENT,
`timestamp` TIMESTAMP NOT NULL,
`oxygen` DOUBLE NOT NULL,
PRIMARY KEY (`ID`))
ENGINE = InnoDB;

CREATE TABLE `fire_alarm` (
`ID` INT NOT NULL AUTO_INCREMENT,
`timestamp` TIMESTAMP NOT NULL,
`alarm` BOOLEAN NOT NULL,
PRIMARY KEY (`ID`))
ENGINE = InnoDB;
